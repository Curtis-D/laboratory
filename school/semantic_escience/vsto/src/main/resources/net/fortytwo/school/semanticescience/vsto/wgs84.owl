<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY wgs84 "http://fortytwo.net/2008/08/wgs84.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://fortytwo.net/2008/08/wgs84.owl#"
     xml:base="http://fortytwo.net/2008/08/wgs84.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wgs84="http://fortytwo.net/2008/08/wgs84.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <rdfs:label>geo</rdfs:label>
        <rdfs:comment
            >This is a direct OWL adaptation of the Geo RDF ontology: http://www.w3.org/2003/01/geo/.  Note: the lat_long property has been omitted.</rdfs:comment>
        <dc:description
            >A vocabulary for representing latitude, longitude and altitude information in the WGS84 geodetic reference datum. Version $Id: wgs84_pos.rdf,v 1.18 2006/02/01 22:01:04 danbri Exp $. See http://www.w3.org/2003/01/geo/ for more details.</dc:description>
        <dc:title
            >WGS84 Geo Positioning: an RDF vocabulary</dc:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://fortytwo.net/2008/08/wgs84.owl#alt -->

    <owl:DatatypeProperty rdf:about="#alt">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>altitude</rdfs:label>
        <rdfs:comment
            >The WGS84 altitude of a SpatialThing (decimal meters \nabove the local reference ellipsoid).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://fortytwo.net/2008/08/wgs84.owl#lat -->

    <owl:DatatypeProperty rdf:about="#lat">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>latitude</rdfs:label>
        <rdfs:comment
            >The WGS84 latitude of a SpatialThing (decimal degrees).</rdfs:comment>
        <rdfs:domain rdf:resource="#SpatialThing"/>
    </owl:DatatypeProperty>
    


    <!-- http://fortytwo.net/2008/08/wgs84.owl#long -->

    <owl:DatatypeProperty rdf:about="#long">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>longitude</rdfs:label>
        <rdfs:comment
            >The WGS84 longitude of a SpatialThing (decimal degrees).</rdfs:comment>
        <rdfs:domain rdf:resource="#SpatialThing"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://fortytwo.net/2008/08/wgs84.owl#Point -->

    <owl:Class rdf:about="#Point">
        <rdfs:label>Point</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SpatialThing"/>
        <rdfs:comment
            >Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT), lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2).</rdfs:comment>
    </owl:Class>
    


    <!-- http://fortytwo.net/2008/08/wgs84.owl#SpatialThing -->

    <owl:Class rdf:about="#SpatialThing">
        <rdfs:label>SpatialThing</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment
            >Anything with spatial extent, i.e. size, shape, or position. e.g. people, places, bowling balls, as well as abstract areas like cubes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
</rdf:RDF>



