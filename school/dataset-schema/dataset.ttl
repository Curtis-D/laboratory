#################################################################


###  http://tw.rpi.edu/schemas/dataset.ttl#category

:category rdf:type owl:DatatypeProperty ;
          
          rdfs:comment "A category describing a dataset."@en ;
          
          rdfs:domain :Dataset ;
          
          rdfs:range rdfs:Literal .





