###  http://tw.rpi.edu/schemas/dataset.ttl#containsDataset

:containsDataset rdf:type owl:InverseFunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:comment "A dataset contained in a data catalog."@en ;
                 
                 rdfs:domain :DataCatalog ;
                 
                 rdfs:range :Dataset ;
                 
                 owl:inverseOf :inCatalog .



###  http://tw.rpi.edu/schemas/dataset.ttl#data

:data rdf:type owl:ObjectProperty ;
      
      rdfs:comment "A downloadable form of this dataset, at a specific location, in a specific format."@en ;
      
      rdfs:range :DataObject ;
      
      rdfs:domain :Dataset .



###  http://tw.rpi.edu/schemas/dataset.ttl#inCatalog

:inCatalog rdf:type owl:FunctionalProperty ,
                    owl:ObjectProperty ;
           
           rdfs:comment "The data catalog which contains a dataset."@en ;
           
           rdfs:range :DataCatalog ;
           
           rdfs:domain :Dataset .



###  http://tw.rpi.edu/schemas/dataset.ttl#spatialScope

:spatialScope rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:comment "The range of spatial applicability of a dataset, e.g. for a dataset of New York weather, the state of New York."@en ;
              
              rdfs:range <http://schema.org/Place> ;
              
              rdfs:domain :Dataset .



###  http://tw.rpi.edu/schemas/dataset.ttl#topic

:topic rdf:type owl:ObjectProperty ;
       
       rdfs:comment "A topic of interest described in a dataset."@en ;
       
       rdfs:range <http://schema.org/Thing> ;
       
       rdfs:domain :Dataset .





#################################################################
#
